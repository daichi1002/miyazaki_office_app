create_table "histories", options: "ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci ROW_FORMAT=DYNAMIC", force: :cascade do |t|
  t.integer "user_id"
  t.string "title"
  t.string "purchase_at"
  t.datetime "created_at"
  t.datetime "updated_at"
end
